{
  "name": "dependency-review-cli",
  "version": "1.0.0",
  "description": "A CLI tool for reviewing dependency changes and vulnerabilities using the GitHub API",
  "license": "Apache-2.0",
  "author": "Nicholas Griffin",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicholasgriffintn/dependency-review-cli"
  },
  "keywords": [
    "dependency-review",
    "cli",
    "github",
    "api",
    "vulnerability",
    "license"
  ],
  "main": "dist/index.js",
  "bin": {
    "dependency-review": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "node --import tsx --test test/*.test.js",
    "test:watch": "node --loader tsx --test --watch test/*.test.js",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@octokit/plugin-retry": "8.0.1",
    "@octokit/request-error": "7.0.0",
    "@octokit/rest": "22.0.0",
    "commander": "14.0.1",
    "yaml": "2.8.1",
    "zod": "4.1.9",
    "lodash": "4.17.20",
    "axios": "0.21.0",
    "serialize-javascript": "5.0.0",
    "ffmpeg-static": "4.4.0",
    "readline-sync": "1.4.10",
    "node-rsa": "1.1.1",
    "spdx-license-list": "6.8.0"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  }
}
